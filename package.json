{
  "name": "homebrew-install",
  "version": "1.0.0",
  "description": "Install Homebrew (on macOS or Linux)",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "start": "dumi dev",
    "build": "dumi build",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\"",
    "make": "node ./bin/update.js"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ineo6/homebrew-install.git"
  },
  "author": "ineo6",
  "license": "BSD-2-Clause",
  "bugs": {
    "url": "https://github.com/ineo6/homebrew-install/issues"
  },
  "homepage": "https://github.com/ineo6/homebrew-install#readme",
  "devDependencies": {
    "babel-plugin-import": "^1.13.3",
    "chalk": "^4.1.0",
    "download-git-repo": "^3.0.2",
    "dumi": "^1.0.12",
    "fs-extra": "^9.0.1",
    "lint-staged": "^10.0.7",
    "ora": "^5.3.0",
    "prettier": "^1.19.1",
    "sitemap": "^6.3.5",
    "yorkie": "^2.0.0"
  },
  "dependencies": {
    "@ant-design/icons": "^4.5.0",
    "antd": "^4.13.1",
    "valine": "^1.4.14"
  }
}
